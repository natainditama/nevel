{
  "name": "nevel",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://nevel.pages.dev/",
  "license": "MIT",
  "description": "React Next Level - Minimalist boilerplate for Next with Tailwind CSS",
  "repository": {
    "type": "git",
    "url": "https://github.com/natainditama/nevel"
  },
  "bugs": {
    "url": "https://github.com/natainditama/nevel/issues",
    "email": "natainditama.dev@gmail.com"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "postbuild": "next-sitemap",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "format": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\""
  },
  "dependencies": {
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^2.0.13",
    "@next/font": "13.1.3",
    "next": "13.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.2.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "eslint-config-next": "^13.1.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-testing-library": "^5.10.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "next-sitemap": "^3.1.49",
    "postcss": "^8.4.21",
    "prettier": "2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "not IE 11"
